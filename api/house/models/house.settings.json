{
  "kind": "collectionType",
  "collectionName": "houses",
  "info": {
    "name": "House"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "houses"
    },
    "foods": {
      "via": "house",
      "collection": "food"
    },
    "recipes": {
      "via": "house",
      "collection": "recipe"
    },
    "list_at": {
      "type": "integer",
      "default": 1,
      "required": true,
      "max": 7,
      "min": 1
    },
    "uuid": {
      "type": "string",
      "unique": true
    }
  }
}
